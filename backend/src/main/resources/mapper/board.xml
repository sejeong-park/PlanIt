<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.trip.planit.board.model.mapper.BoardMapper">
	<insert id="insertArticle" parameterType="boardDto">
		insert into board(title, content) values(#{title}, #{content}) 
	</insert>
	<select id="selectAllArticles" resultType="boardDto">
		select * from board;
	</select>
</mapper>