<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
	PUBLIC "-mybatis.org/DTD mapper 3.0//EN"
	"https://mybatis.org/dtd/mybatis-3-mapper.dtd"
>
<mapper namespace="com.trip.planit.model.mapper.UserMapper">
	<select id="selectByUser" parameterType="userDto" resultType="userDto">
		SELECT * FROM user where id=#{userId} and password=#{userPwd}
	</select>
	
	
	<select id="listUser" resultType="userDto">
		select * from user
	</select>
</mapper>